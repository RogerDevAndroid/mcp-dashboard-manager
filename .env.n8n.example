# ========================================
# N8N MCP Orchestrator - Environment Variables
# ========================================

# N8N Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/ghl-orchestrator

# GoHighLevel API
GHL_API_BASE_URL=https://services.leadconnectorhq.com
GHL_PRIVATE_TOKEN=your_ghl_private_integration_token_here
GHL_LOCATION_ID=your_ghl_location_id_here

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
