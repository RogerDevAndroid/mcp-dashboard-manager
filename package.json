{
  "name": "mcp-dashboard",
  "version": "1.0.0",
  "description": "Dashboard para gestionar MCP Servers y Agentes Orquestadores",
  "main": "agents/orchestrator-agent.js",
  "scripts": {
    "build:orchestrator": "cd agents && tsc orchestrator-agent.ts",
    "start:orchestrator": "node agents/orchestrator-agent.js",
    "dev": "next dev",
    "build": "npm run build:orchestrator && next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "npm run build:orchestrator && echo 'Setup complete!'",
    "test:orchestrator": "USER_ID=test_user node agents/orchestrator-agent.js"
